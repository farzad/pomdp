# Build pomdp-solve

.PHONY: all clean

all: clean
	(cd pomdp-solve && CXX="$(CXX)" CC="$(CC)" CFLAGS="$(CFLAGS) $(CPICFLAGS)" CXXFLAGS="$(CXXFLAGS)" AR="$(AR)" RANLIB="$(RANLIB)" $(MAKE))

clean:
	(cd pomdp-solve && CXX="$(CXX)" CC="$(CC)" CFLAGS="$(CFLAGS) $(CPICFLAGS)" CXXFLAGS="$(CXXFLAGS)" AR="$(AR)" RANLIB="$(RANLIB)" $(MAKE) clean)


